<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <!--====== Working Effect Food & Effect Working Bionics ======-->

    <EffecterDef>
        <defName>AlienRecipeNormal</defName>
        <children>
            <li>
                <subEffecterClass>SubEffecter_SprayerContinuous</subEffecterClass>
                <scale>
                    <min>0.3</min>
                    <max>0.7</max>
                </scale>
                <ticksBetweenMotes>20</ticksBetweenMotes>
                <positionLerpFactor>0.6</positionLerpFactor>
                <positionRadius>0.6</positionRadius>
                <moteDef>RacePreparation</moteDef>
            </li>
        </children>
    </EffecterDef>

    <ThingDef ParentName="FoodBitBase">
        <defName>RacePreparation</defName>
        <graphicData>
            <texPath>Things/Mote/RecipeMotes/Work/RecipePreparation</texPath>
            <shaderType>Mote</shaderType>
        </graphicData>
    </ThingDef>


