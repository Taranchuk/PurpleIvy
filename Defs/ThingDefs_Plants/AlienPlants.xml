<Defs>

  <ThingDef Name="StrangePlants" Abstract="True">
    <category>Plant</category>
    <thingClass>Plant</thingClass>
    <altitudeLayer>LowPlant</altitudeLayer>
    <useHitPoints>True</useHitPoints>
    <statBases>
      <Flammability>1</Flammability>
    </statBases>
    <tickerType>Long</tickerType>
    <selectable>False</selectable>
    <neverMultiSelect>True</neverMultiSelect>
    <drawerType>MapMeshOnly</drawerType>
    <pathCostIgnoreRepeat>false</pathCostIgnoreRepeat>
    <graphicData>
      <graphicClass>Graphic_Random</graphicClass>
      <shaderType>CutoutPlant</shaderType>
    </graphicData>
    <ingestible>
      <foodType>Plant</foodType>
      <preferability>RawBad</preferability>
    </ingestible>
    <plant>
      <fertilityMin>0.7</fertilityMin>
      <fertilitySensitivity>1.0</fertilitySensitivity>
      <soundHarvesting>Harvest_Standard</soundHarvesting>
      <soundHarvestFinish>Harvest_Standard_Finish</soundHarvestFinish>
      <sowWork>170</sowWork>
      <harvestWork>275</harvestWork>
      <visualSizeRange>
        <min>0.3</min>
        <max>1.00</max>
      </visualSizeRange>
      <topWindExposure>0.1</topWindExposure>
    </plant>
  </ThingDef>



  <ThingDef ParentName="StrangePlants">
    <defName>PlantCreepyTulip</defName>
    <label>Creepy Tulip</label>
    <description>Little blue and green plant with large leaves and it's specific tulip like structure in middle. This plant produce very big and tasteful structure on top. After this structure plant have it's name it contains six large seeds with good nutrient yield.</description>
    <statBases>
      <MaxHitPoints>65</MaxHitPoints>
      <Nutrition>0.15</Nutrition>
      <Beauty>20</Beauty>
    </statBases>
    <graphicData>
      <texPath>Things/Item/Plants/CreepyTulip</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <selectable>true</selectable>
    <pathCost>10</pathCost>
    <ingestible />
    <plant>
      <fertilitySensitivity>1.0</fertilitySensitivity>
      <growDays>5</growDays>
      <immatureGraphicPath>Things/Item/Plants/ImmaturePlants/CreepyTulip</immatureGraphicPath>
      <dieIfLeafless>true</dieIfLeafless>
      <harvestTag>Standard</harvestTag>
      <harvestedThingDef>CreepyTulip</harvestedThingDef>
      <harvestYield>6</harvestYield>
      <sowTags>
        <li>Ground</li>
        <li>Hydroponic</li>
      </sowTags>
      <sowMinSkill>2</sowMinSkill>
      <sowResearchPrerequisites>
        <li>BasicAlienPlants</li>
      </sowResearchPrerequisites>
      <topWindExposure>0.2</topWindExposure>
      <visualSizeRange>
        <min>0.3</min>
        <max>1.0</max>
      </visualSizeRange>
    </plant>
  </ThingDef>

